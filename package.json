{
    "name": "typescript_project_framework",
    "version": "1.0.0",
    "description": "TypeScript NodeJS Project Framework",
    "main": "index.js",
    "scripts": {
        "tsc": "tsc",
        "build":"npx webpack --config webpack.app_local.js --progress",
        "buildDgf": "npx webpack --config webpack.app_dgf.js --progress",
        "buildGsk": "npx webpack --config webpack.app_gsk.js --progress",
        "buildAz": "npx webpack --config webpack.app_az.js --progress",
        "buildConsumeDgf": "npx webpack --config webpack.im_consume_ana_dgf.js --progress",
        "buildConsumeGsk": "npx webpack --config webpack.im_consume_ana_gsk.js --progress",
        "buildConsumeAz": "npx webpack --config webpack.im_consume_ana_az.js --progress",
        "buildLocal": "npx webpack --config webpack.app_local.js --progress",
        "buildImConusme": "npx webpack --config webpack.im_consume_ana_dgf.js --progress",
        "compile": "webpack --progress --watch",
        "start": "tsc && node ./dist/main.js && echo $! > .pid",
        "startLocal": "pm2 start app-local.json --env && echo $! > consume.pid",
        "startGsk": "pm2 start app.json --env dgf && echo $! > consume.pid",
        "startGsk1": "pm2 start app-gsk.json --env gsk && echo $! > consume.pid",
        "startAz": "pm2 start app-az.json --env az && echo $! > consume.pid",
        "consumeStart-dhl": "pm2 start consume-app.json --env dhl && echo $! > consume.pid",
        "consumeStart-gsk": "pm2 start consume-app-gsk.json --env gsk && echo $! > consume.pid",
        "consumeStart-az": "pm2 start consume-app-az.json --env az && echo $! > consume.pid",
        "stop": "kill $(cat .pid)",
        "prod": "tsc && node ./dist/main.js",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "body-parser": "^1.19.0",
        "convert-excel-to-json": "^1.7.0",
        "cors": "^2.8.5",
        "cron": "^1.8.2",
        "dateformat": "^5.0.2",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-session": "^1.17.0",
        "jsonwebtoken": "^8.5.1",
        "kafkajs": "^1.12.0",
        "keycloak-connect": "^8.0.1",
        "moment": "^2.24.0",
        "multer": "^1.4.3",
        "mysql": "^2.18.1",
        "mysql2": "^2.3.3",
        "sequelize": "6.9.0",
        "sequelize-typescript": "^2.1.1",
        "winston": "^3.2.1",
        "xlsx": "^0.17.4",
        "xlsx-to-json-lc": "^0.5.0"
    },
    "devDependencies": {
        "@types/bluebird": "^3.5.28",
        "@types/dateformat": "^5.0.0",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.1",
        "@types/express-session": "^1.15.16",
        "@types/jsonwebtoken": "^8.5.6",
        "@types/kafkajs": "^1.9.0",
        "@types/keycloak-connect": "^4.5.1",
        "@types/moment": "^2.13.0",
        "@types/mongodb": "^3.3.10",
        "@types/validator": "^10.11.3",
        "awesome-typescript-loader": "^5.2.1",
        "babel-loader": "^8.0.6",
        "sequelize-auto": "^0.7.6",
        "ts-loader": "^6.2.1",
        "typescript": "^3.7.5",
        "webpack": "^5.65.0",
        "webpack-cli": "^4.9.1",
        "webpack-node-externals": "^1.7.2"
    }
}
